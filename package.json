{
  "name": "new-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "coverage": "yarn test -- --coverage",
    "test": "tsnd -T node_modules/jest/bin/jest.js ",
    "test:ls": "tsnd --quiet ./getTestFiles.ts ",
    "test:watch": "tsnd -T node_modules/jest/bin/jest.js --watch",
    "clean": "rimraf dist",
    "transpile": "tsc --noEmit false && tsc-alias",
    "copy-reference": "copyfiles -f src/reference/* dist/reference/",
    "build": "npm-run-all clean transpile copy-reference",
    "start": "tsnd --respawn -r tsconfig-paths/register  --transpile-only  ./src/index.ts",
    "postinstall": "husky install",
    "types": "openapi-typescript src/reference/openapi.yml --output src/schema.ts "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@appquality/tryber-database": "^0.18.1",
    "@appquality/clickday-database": "^0.9.0",
    "@appquality/wp-auth": "^1.0.7",
    "body-parser": "^1.19.1",
    "comment-parser": "^1.3.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "glob": "^8.0.3",
    "jsonwebtoken": "^9.0.0",
    "openapi-backend": "^3.9.2",
    "php-unserialize": "^0.0.1",
    "wordpress-hash-node": "^1.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.50",
    "@swc/jest": "^0.2.26",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/glob": "^8.1.0",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mysql": "^2.15.20",
    "@types/supertest": "^2.0.11",
    "@types/wordpress-hash-node": "^1.0.0",
    "copyfiles": "^2.4.1",
    "husky": "^8.0.3",
    "jest": "^29.0.0",
    "jest-cli": "^29.0.0",
    "jest-expect-message": "^1.1.3",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^5.1.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
